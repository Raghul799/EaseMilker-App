# Copy to .env for local dev only (do not commit real secrets)
FIREBASE_PROJECT_ID=your-firebase-project-id
GOOGLE_APPLICATION_CREDENTIALS=../service-accounts/firebase-admin.json

# MQTT broker
MQTT_BROKER_URL=mqtts://your-emqx-host:8883
MQTT_USERNAME=your-mqtt-username
MQTT_PASSWORD=your-mqtt-password
MQTT_CLIENT_ID=easemilker-ingestor-local
MQTT_TLS=true
MQTT_KEEPALIVE_SECONDS=60

# Service options
PORT=8080
LOG_LEVEL=info
